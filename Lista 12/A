using System;

namespace Exercicios
{
    class Program
    {
        public static void Main()
        {
        	
        //Crie um array de inteiros e use o foreach para imprimir cada elemento na tela.
        
        int [] numeros = new int[5];
        
        numeros[0] = 10;
        numeros[1] = 20;
        numeros[2] = 30;
        numeros[3] = 40;
        numeros[4] = 50;
        
        Console.WriteLine("NÃºmeros do array:\n");
        
        foreach(int numero in numeros){

        	Console.WriteLine(numero);
        }

            Console.WriteLine("");
            Console.WriteLine("Pressione qualquer tecla para fechar.");
            Console.ReadKey();
        }
    }
}
